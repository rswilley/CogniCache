@page "/"
@using CogniCache.Infrastructure.Services
@using CogniCache.Shared.Components
@using CogniCache.Shared.Services

@inject IFormFactor FormFactor
@inject IConfigurationService configService

<Heading Size="HeadingSize.Is1" Margin="Margin.Is3.OnY">
    Dashboard
</Heading>

<Row>
    <Column Margin="Margin.Is3.FromBottom">
        @* <Editor Title="Quick Note"/> *@
    </Column>
</Row>

@code {
    private string factor => FormFactor.GetFormFactor();
    private string platform => FormFactor.GetPlatform();

    protected override void OnInitialized()
    {
        configService.LoadConfiguration();
    }
}
